{
  "name": "editorjs-font-size",
  "version": "0.0.1",
  "description": "This is a font size plugin created for editorjs",
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=12.20.0"
  },
  "scripts": {
    "build": "npm run rollup && npm run post:build",
    "post:build": "node ./scripts/build.js",
    "rollup": "rm -rf dist && rollup -c --bundleConfigAsCjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaisriramr/editorjs-font-size"
  },
  "keywords": [
    "editorjs",
    "typescript",
    "font-family",
    "plugin"
  ],
  "author": "jaisriramr",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup": "^4.9.5",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@editorjs/editorjs": "^2.28.2"
  }
}
